<% provide(:title, 'Project Details') %>
<%- model_class = Project -%>
<div class="page-header">
   <h1><%=t '.title', :default => model_class.model_name.human %></h1>
</div>

<p id="notice"><%= notice %></p>

<dl class="dl-horizontal">

  <dt><strong><%= model_class.human_attribute_name(:code) %>:</strong></dt>
  <dd><%= @project.code %></dd>


  <dt><strong><%= model_class.human_attribute_name(:name) %>:</strong></dt>
  <dd><%= @project.name %></dd>


  <dt><strong><%= model_class.human_attribute_name(:customer_ref) %>:</strong></dt>
  <dd><%= @project.customer_ref %></dd>



  <dt><strong><%= model_class.human_attribute_name(:company) %>:</strong><dt>
  <dd><%= @project.company_id %></dd>



 <dt><strong><%= model_class.human_attribute_name(:start_at) %>:</strong></dt>
  <dd><%= @project.start_at %></dd>



  <dt><strong><%= model_class.human_attribute_name(:end_at) %>:</strong></dt>
  <dd><%= @project.end_at %></dd>



  <dt><strong><%= model_class.human_attribute_name(:value) %>:</strong></dt>
  <dd><%= @project.value %></dd>
</dl>

<%= link_to 'Edit', edit_project_path(@project) %> |
<%= link_to 'Back', projects_path %>
